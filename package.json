{
  "name": "jogga-website",
  "version": "0.0.0",
  "description": "Jogga's personal site",
  "scripts": {
    "scss:build": "node-sass -r --output-style compressed src/scss -o static/css",
    "scss:dev": "npm run scss:build && npm run scss:build -- -w",
    "hugo": "hugo",
    "hugo:dev": "hugo server -D",
    "dev": "concurrently --kill-others \"npm run scss:dev\" \"npm run hugo:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jogga/jogga-website.git"
  },
  "keywords": [
    "portfolio",
    "hugo",
    "hugo-site"
  ],
  "author": "Joachim Fr√∂stl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jogga/jogga-website/issues"
  },
  "homepage": "https://jogga.co",
  "dependencies": {
    "concurrently": "^3.5.1",
    "node-sass": "^4.9.0"
  }
}
